cmake_minimum_required(VERSION 3.0)
project(ApiClient)

set(CMAKE_CXX_FLAGS "${CMAKE_CXX_FLAGS} -std=c++11")

set(SOURCE_FILES main.cpp)
add_executable(ApiClient ${SOURCE_FILES})

target_link_libraries( ${PROJECT_NAME}
                        -lSDL2main
                        -lSDL2
                        -lSDL2_ttf
                        -lSDL2_gfx
					  )
